{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "Chrome >= 40",
            "FireFox >= 44",
            "Safari >= 7",
            "Explorer 10",
            "last 4 Edge versions"
          ]
        },
        "useBuiltIns": "usage"
      }
    ],
    "@babel/preset-react"
  ],
  "env": {
    "docs": {
      "plugins": [
        "add-react-displayname",
        [
          "babel-plugin-react-docgen",
          {
            "DOC_GEN_COLLECTION_NAME": "REACT_DOCS"
          }
        ]
      ]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from"
  ]
}
